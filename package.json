{
  "name": "ods-learncode",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "cors": "^2.8.3",
    "express": "^4.15.3",
    "history": "^4.6.2",
    "jquery": "^3.2.1",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "mongoose": "^4.11.0",
    "path": "^0.12.7",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.1",
    "react-truncate": "^2.1.4",
    "semantic-ui-css": "^2.2.10",
    "semantic-ui-react": "^0.70.0"
  },
  "devDependencies": {
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2",
    "react-scripts": "1.0.7"
  },
  "scripts": {
    "express": "node ./src/server/server.js",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js express",
